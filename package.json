{
  "name": "cra-twin-mobx-boiler",
  "version": "0.1.0",
  "private": true,
  "author": "zerone2",
  "description": "CRA + twin.macro + MobX boilerplate",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
